@page
@model Taste.Pages.Admin.Users.IndexModel
@{
    ViewData["Title"] = "All Users";
}

<div class="border backgroundWhite container">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Users List</h2>
        </div>

        <div class="col-6 text-right">
            <a asp-area="Identity" asp-page="/Account/Register" class="btn btn-primary"><i class="fas fa-plus">&nbsp;Create New</i></a>
           
        </div>
    </div>
    <br /><br />
    <table id="DT_load" class="table table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Full Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>


@section Scripts{
    <script src="~/js/appuser.js"></script>
}